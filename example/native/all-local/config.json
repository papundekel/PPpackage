{
    "repository_drivers": {
        "pacman": {
            "package": "PPpackage_pacman"
        },
        "AUR": {
            "package": "PPpackage_AUR"
        },
        "conan": {
            "package": "PPpackage_conan"
        },
        "PP": {
            "package": "PPpackage_PP"
        }
    },
    "requirement_translators": {
        "archlinux": {
            "package": "PPpackage_translator_archlinux"
        },
        "conan": {
            "package": "PPpackage_translator_conan"
        },
        "PP": {
            "package": "PPpackage_translator_PP"
        }
    },
    "installers": {
        "pacman": {
            "package": "PPpackage_installer_pacman",
            "parameters": {
                "containerizer": {
                    "url": "unix://$XDG_RUNTIME_DIR/podman/podman.sock"
                }
            }
        },
        "simple": {
            "package:": "PPpackage_installer_simple"
        }
    },
    "repositories": [
        {
            "driver": "pacman",
            "parameters": {
                "database_path": "tmp/database/archlinux-core",
                "header": "core",
                "include": "/etc/pacman.d/mirrorlist"
            }
        },
        {
            "driver": "pacman",
            "parameters": {
                "database_path": "tmp/database/archlinux-extra",
                "header": "extra",
                "include": "/etc/pacman.d/mirrorlist"
            }
        },
        {
            "driver": "AUR"
        },
        {
            "driver": "conan",
            "parameters": {
                "cache_path": "tmp/cache/conan-conancenter",
                "url": "https://center.conan.io",
                "verify_ssl": true
            }
        },
        {
            "driver": "PP",
            "parameters": {
                "database_path": "tmp/databaes/PP",
                "cache_path": "tmp/cache/PP"
            }
        }
    ]
}
