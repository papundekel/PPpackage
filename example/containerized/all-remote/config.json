{
    "installers": {
        "pacman": {
            "package": "PPpackage_installer_pacman",
            "parameters": {
                "containerizer": {
                    "url": "unix:///run/podman/podman.sock",
                    "path_translations": [
                        {
                            "containerizer": "/tmp",
                            "container": "/mnt/workdir"
                        },
                        {
                            "containerizer": "$ROOT",
                            "container": "/mnt/root"
                        }
                    ]
                }
            }
        },
        "simple": {
            "package:": "PPpackage_installer_simple"
        }
    },
    "repositories": [
        {
            "name:": "archlinux-core",
            "url": "http://archlinux-core:8888"
        },
        {
            "name:": "archlinux-extra",
            "url": "http://archlinux-extra:8888"
        },
        {
            "name:": "AUR",
            "url": "http://AUR:8888"
        },
        {
            "name:": "conan-conancenter",
            "url": "http://conan-conancenter:8888"
        },
        {
            "name:": "PP",
            "url": "http://pp:8888"
        }
    ]
}
