{
    "product_cache_path": "tmp/cache/product",
    "requirement_translators": {
        "pacman": {
            "package": "PPpackage.translator.pacman"
        },
        "conan": {
            "package": "PPpackage.translator.conan"
        },
        "PP": {
            "package": "PPpackage.translator.PP"
        }
    },
    "installers": {
        "pacman": {
            "package": "PPpackage.installer.pacman",
            "parameters": {
                "containerizer": {
                    "url": "unix://$XDG_RUNTIME_DIR/podman/podman.sock"
                }
            }
        },
        "simple": {
            "package": "PPpackage.installer.simple"
        }
    },
    "repositories": [
        {
            "url": "http://localhost:8880",
            "cache_path": "tmp/cache/http/archlinux-core"
        },
        {
            "url": "http://localhost:8881",
            "cache_path": "tmp/cache/http/archlinux-extra"
        },
        {
            "url": "http://localhost:8882",
            "cache_path": "tmp/cache/http/AUR"
        },
        {
            "url": "http://localhost:8883",
            "cache_path": "tmp/cache/http/conan-conancenter"
        },
        {
            "url": "http://localhost:8884",
            "cache_path": "tmp/cache/http/PP"
        }
    ]
}
