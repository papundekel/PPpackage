{
    "product_cache_path": "tmp/cache/product",
    "repository_drivers": {
        "pacman": {
            "package": "PPpackage.repository_driver.pacman"
        },
        "AUR": {
            "package": "PPpackage.repository_driver.AUR"
        },
        "conan": {
            "package": "PPpackage.repository_driver.conan"
        },
        "PP": {
            "package": "PPpackage.repository_driver.PP"
        }
    },
    "requirement_translators": {
        "pacman": {
            "package": "PPpackage.translator.pacman"
        },
        "conan": {
            "package": "PPpackage.translator.conan"
        },
        "PP": {
            "package": "PPpackage.translator.PP"
        }
    },
    "installers": {
        "pacman": {
            "package": "PPpackage.installer.pacman",
            "parameters": {
                "containerizer": {
                    "url": "unix://$XDG_RUNTIME_DIR/podman/podman.sock"
                }
            }
        },
        "simple": {
            "package": "PPpackage.installer.simple"
        }
    },
    "repositories": [
        {
            "driver": "pacman",
            "parameters": {
                "database_path": "tmp/database/archlinux-core",
                "repository": "core",
                "mirrorlist": [
                    "http://ftp.sh.cvut.cz/arch/core/os/x86_64"
                ]
            }
        },
        {
            "driver": "pacman",
            "parameters": {
                "database_path": "tmp/database/archlinux-extra",
                "repository": "extra",
                "mirrorlist": [
                    "http://ftp.sh.cvut.cz/arch/core/os/x86_64"
                ]
            }
        },
        {
            "driver": "AUR"
        },
        {
            "driver": "conan",
            "parameters": {
                "cache_path": "tmp/cache/repository/conan-conancenter",
                "url": "https://center.conan.io",
                "verify_ssl": true
            }
        },
        {
            "driver": "PP",
            "parameters": {
                "database_path": "tmp/database/PP",
                "cache_path": "tmp/cache/repository/PP"
            }
        }
    ]
}
