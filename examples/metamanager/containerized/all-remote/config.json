{
    "translators": {
        "pacman": {
            "package": "PPpackage.translator.pacman"
        },
        "conan": {
            "package": "PPpackage.translator.conan"
        }
    },
    "installers": {
        "pacman": {
            "package": "PPpackage.installer.pacman",
            "parameters": {
                "containerizer": {
                    "url": "unix:///run/podman/podman.sock",
                    "path_translations": [
                        {
                            "containerizer": "$ROOT",
                            "container": "/mnt/root"
                        }
                    ]
                }
            }
        }
    },
    "repositories": [
        {
            "name": "archlinux-core",
            "url": "http://archlinux-core:8888"
        },
        {
            "name": "archlinux-extra",
            "url": "http://archlinux-extra:8888"
        },
        {
            "name": "AUR",
            "url": "http://AUR:8888"
        },
        {
            "name": "conancenter",
            "url": "http://conancenter:8888"
        }
    ]
}
