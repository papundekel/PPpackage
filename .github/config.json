{
    "product_cache_path": "/mnt/cache/product/",
    "repository_drivers": {
        "pacman": {
            "package": "PPpackage.repository_driver.pacman"
        },
        "AUR": {
            "package": "PPpackage.repository_driver.AUR"
        },
        "conan": {
            "package": "PPpackage.repository_driver.conan"
        },
        "PP": {
            "package": "PPpackage.repository_driver.PP"
        }
    },
    "requirement_translators": {
        "pacman": {
            "package": "PPpackage.translator.pacman"
        },
        "conan": {
            "package": "PPpackage.translator.conan"
        },
        "PP": {
            "package": "PPpackage.translator.PP"
        }
    },
    "installers": {
        "pacman": {
            "package": "PPpackage.installer.pacman",
            "parameters": {
                "containerizer": {
                    "url": "unix:///run/podman/podman.sock",
                    "path_translations": [
                        {
                            "containerizer": "$CONTAINERIZER_ROOT",
                            "container": "/mnt/root"
                        }
                    ]
                }
            }
        },
        "simple": {
            "package": "PPpackage.installer.simple"
        }
    },
    "repositories": [
        {
            "driver": "pacman",
            "parameters": {
                "database_path": "/mnt/repositories/database/archlinux-core/",
                "mirrorlist": [
                    "https://geo.mirror.pkgbuild.com/core/os/x86_64"
                ]
            },
            "formula_cache_path": "/mnt/cache/formula/archlinux-core.db",
            "translator_data_cache_path": "/mnt/cache/translator-data/archlinux-core.db"
        },
        {
            "driver": "pacman",
            "parameters": {
                "database_path": "/mnt/repositories/database/archlinux-extra/",
                "mirrorlist": [
                    "https://geo.mirror.pkgbuild.com/extra/os/x86_64"
                ]
            },
            "formula_cache_path": "/mnt/cache/formula/archlinux-extra.db",
            "translator_data_cache_path": "/mnt/cache/translator-data/archlinux-extra.db"
        },
        {
            "driver": "AUR",
            "formula_cache_path": "/mnt/cache/formula/AUR.db",
            "translator_data_cache_path": "/mnt/cache/translator-data/AUR.db"
        },
        {
            "driver": "conan",
            "parameters": {
                "database_path": "/mnt/repositories/database/conancenter/",
                "url": "https://center.conan.io",
                "verify_ssl": true
            },
            "formula_cache_path": "/mnt/cache/formula/conancenter.db",
            "translator_data_cache_path": "/mnt/cache/translator-data/conancenter.db"
        },
        {
            "driver": "PP",
            "parameters": {
                "database_path": "/mnt/repositories/database/PP",
                "cache_path": "/mnt/cache/repository/PP"
            },
            "formula_cache_path": "/mnt/cache/formula/PP.db",
            "translator_data_cache_path": "/mnt/cache/translator-data/PP.db"
        }
    ]
}
